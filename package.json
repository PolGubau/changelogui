{
	"name": "@polgubau/changelogui",
	"version": "1.0.2",
	"private": false,
	"description": "",
	"license": "MIT",
	"author": "Pol Gubau Amores",
	"type": "module",
	"main": "./bin/changelogui.js",
	"bin": {
		"changelogui": "./bin/changelogui.js"
	},
	"scripts": {
		"build": "vite build",
		"check": "biome lint --write .",
		"commit": "git-cz",
		"dev": "vite",
		"format": "biome format --write ./src",
		"format-check": "biome format ./src",
		"lint": "biome check .",
		"lint-check": "biome lint ./src",
		"lint-fix": "biome check --write .",
		"release": "semantic-release"
	},
	"config": {
		"commitizen": {
			"path": "git-cz"
		}
	},
	"dependencies": {
		"@octokit/rest": "^21.1.0",
		"@polgubau/semantic-release-config": "^1.0.0",
		"autoprefixer": "^10.4.20",
		"chalk": "^5.4.1",
		"commander": "^13.0.0",
		"postcss": "^8.4.49",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"tailwindcss": "^3.4.17",
		"vite": "^6.0.7"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@polgubau/biome-config": "^1.0.0",
		"@polgubau/git-cz-config": "^1.0.0",
		"git-cz": "^4.9.0"
	},
	"packageManager": "pnpm@9.15.0",
	"publishConfig": {
		"access": "public"
	}
}
